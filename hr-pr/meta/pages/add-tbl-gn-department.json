{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "type": "template-form",
                  "name": "form0",
                  "components": [
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Department Id",
                              "component": "Department_ID",
                              "name": "Department_IDLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 5,
                              "type": "textbox",
                              "name": "Department_ID",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "value": "${tblgndepartment.Department_ID}"
                            },
                            {
                              "type": "required-validator",
                              "name": "Department_IDRequiredValidator",
                              "component": "Department_ID",
                              "text": "Department_ID is required",
                              "style": {
                                "position": "absolute"
                              }
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Deparment Name",
                              "component": "Deparment_Name",
                              "name": "Deparment_NameLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 15,
                              "type": "textbox",
                              "name": "Deparment_Name",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblgndepartment.Deparment_Name}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "components": [
                        {
                          "type": "col",
                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Description",
                              "component": "Description",
                              "name": "DescriptionLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "string",
                              "maxLength": 150,
                              "type": "textbox",
                              "name": "Description",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${tblgndepartment.Description}"
                            }
                          ]
                        }
                      ]
                    },
                    {
                      "type": "row",
                      "components": [
                        {
                          "type": "col",
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new TblGnDepartment!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "DB_At_VDC2.createTblGnDepartment",
                        "parameters": [
                          {
                            "name": "TblGnDepartment",
                            "value": "${tblgndepartment}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${tblgndepartment}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${tblgndepartment}",
                  "visible": "${tblgndepartment != null}",
                  "itemType": "CanErpHrPr.Models.DbAtVdc2.TblGnDepartment"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "tblgndepartment",
        "type": "set",
        "value": "new CanErpHrPr.Models.DbAtVdc2.TblGnDepartment()",
        "valueType": "CanErpHrPr.Models.DbAtVdc2.TblGnDepartment"
      }
    ]
  },
  "name": "Add Tbl Gn Department",
  "layout": "Main"
}